package com.simpleexample.Projrct;

public class Employee {
	String empId;
	String Ename;
	String Location;
	
	public void setEmpId(String empId) {
		this.empId = empId;
	}
	
	public void setEname(String ename) {
		Ename = ename;
	}
	
	public void setLocation(String location) {
		Location = location;
	}
	
	public void display() {
		System.out.println("Display() calling");
		
	}

}
