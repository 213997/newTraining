package com.labour.laburservice.entity;

import jakarta.persistence.Entity;

@Entity
public class Labour {
	
	
	private int laburId;
	private String name;
	private long phoneno;
	private String speclization;
	private long AdharNo;
	private String location;
	private int experience;
	private int payperday;
	private boolean avlable;
	
	public Labour() {
		
	}
	public Labour(int laburId, String name, long phoneno, String speclization, long adharNo, String location,
			int experience, int payperday, boolean avlable) {
		
		this.laburId = laburId;
		this.name = name;
		this.phoneno = phoneno;
		this.speclization = speclization;
		AdharNo = adharNo;
		this.location = location;
		this.experience = experience;
		this.payperday = payperday;
		this.avlable = avlable;
	}
	
	
	

}
